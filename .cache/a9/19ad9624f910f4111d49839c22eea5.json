 }\n  \n  render() {\n    const template = html`\n      <cb-app-header\n        user=\"${JSON.stringify(Auth.currentUser)}\"\n      ></cb-app-header>\n      <div class=\"favourites\">\n      <div class=\"page-content\">\n      <h1>Your favourite cupcakes!</h1>\n       <div class=\"products-grid\">\n          ${\n            this.favProducts == null\n              ? html` \n              <div class=\"loading\">\n              <img src=\"../../images/loading-animation.gif\"/> </div> `\n              : html`\n                  ${this.favProducts.map(\n                    (product) => html`\n                      <cb-favourites\n                        class=\"product-card\"\n                        id=\"${product._id}\"\n                        productName=\"${product.productName}\"\n                        price=\"${product.price}\"\n                        image=\"${product.image}\"\n                      >\n                      </cb-favourites>\n                    `\n                  )}\n                `}\n        </div>\n      </div>\n      </div>\n      <cb-app-footer></cb-app-footer>\n    `;\n    render(template, App.rootEl);\n  }\n}\n\n\nexport default new FavouriteProductsView()"},"lineCount":null}},"error":null,"hash":"bf5aa506dbbb8702fdd53ce15b8673c6","cacheData":{"env":{}}}